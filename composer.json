{
    "name": "blendbyte/nova-items-field",
    "description": "Nova field to handle array columns",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^5.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "NovaItemsField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaItemsField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
